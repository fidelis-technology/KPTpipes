<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Equipment Code -->
        <record id="seq_equipment_code" model="ir.sequence">
            <field name="name">Equipment Code Sequence</field>
            <field name="code">equipment.master</field>
            <field name="prefix">EQ</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data noupdate="1">
        <!-- Sequence for Product Issue -->
        <record id="seq_product_issue" model="ir.sequence">
            <field name="name">Product Issue Sequence</field>
            <field name="code">product.issue</field>
            <field name="prefix">PI</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>
